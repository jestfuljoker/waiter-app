{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"baseUrl": "./",
		"paths": {
			"~/*": [
				"src/*"
			]
		},
	}
}
